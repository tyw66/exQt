/*********************
* 浮动窗体
**********************/
QDockWidget {
    border: 2px solid #CCC;
    color:#FFF;
    font-size:1.2em;
    font: bold;
}

QDockWidget::title {
    text-align: left;
    padding: 5px 5px;
    background: transparent;/*#08D*/
    border-radius:3px;
    titlebar-close-icon: url(:/qss/resource/skin/lightblue/Close.png);
    titlebar-normal-icon: url(:/qss/resource/skin/lightblue/Max.png);
}


/*=====================================================================*/
/*QGroupBox*/
/*=====================================================================*/
QGroupBox{
    margin-top:0.5em;
    border:1px solid #242A28;
    border-radius:4px;
    background:transparent;
}
QGroupBox::title{
    left:10px;
    subcontrol-origin:margin;
    subcontrol-position:top left;
    margin:0px;
    padding:0px;
    color:#FEFFAB;
    background:#54575B;
}

/*=====================================================================*/
/*QMenuBar*/
/*=====================================================================*/
QMenuBar{

}

QMenuBar::item{
    padding:3px 10px;
}

QMenuBar::item:selected{
    border:1px solid #242A28;
	color:#000000;
    background:#F8CE72;
}

/*=====================================================================*/
/*QMenu*/
/*=====================================================================*/
QMenu,QMenu:disabled{
	border:1px solid #242A28;
    margin:0px;
}

QMenu::item:selected{
    border:1px solid #242A28;
	color:#000000;
    background:#F8CE72;
}

QMenu::item{
    padding:3px 20px;
}

QMenu::indicator{
    width:13px;
    height:13px;
}
QMenu::separator{
    height:1px;
    background:#242A28;
}


/*=====================================================================*/
/*QPushButton*/
/*=====================================================================*/
QPushButton{
    min-height:1.2em;
    min-width:80px; /*×îÐ¡¿í¶ÈÔõÃŽ¶š£¿*/
    
    background:#6A6E73;
    border: 1px solid #242A28;
    /*
    border: 1px solid #353535;
    padding: 2px;
    border-radius: 5px;
    background:qlineargradient(spread:pad,x1:0,y1:0,x2:0,y2:1,stop:0 #4D4D4D,stop:1 #383838);*/
}
