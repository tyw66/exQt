/*=====================================================================*/
QComboBox{
    border:1px solid #242A28;
    border-radius:1px;
    padding:2px;
    background:#6A6E73;

    selection-background-color:#F8CE72;
    selection-color:#000000;

/*        combobox-popup: 0;*/
}

QComboBox:focus{
        border:1px solid #F8CE72;
}

QComboBox:hover{
    border:1px solid #F8CE72;
}

QComboBox::drop-down{
    subcontrol-origin:padding;
    subcontrol-position:top right;
    width:15px;
    padding:2px;
}

QComboBox::menu{
        background:#F8CE72;
}
