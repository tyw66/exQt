}

/*=====================================================================*/
/*QSlider*/
/*=====================================================================*/
QSlider{

}

QSlider::groove:horizontal,
QSlider::groove:vertical{
    height:4px;
    border-radius:4px;
}

QSlider::handle{
    width:10px;
    height:25px;
    margin-top:-6px;
    margin-bottom:-6px;
    border-radius:1px;
    background:#FFB56B;
}

QSlider::add-page:horizontal,
QSlider::add-page:vertical{
    background:#DDDDDD;
    height:2px;
    border-radius:2px;
}

QSlider::sub-page:horizontal,
QSlider::sub-page:vertical{
    background:#FFB56B;
    height:4px;
    border-radius:0px;
}


QSlider::handle:disabled,
QSlider::sub-page:horizontal:disabled,
QSlider::sub-page:vertical:disabled{
    background:#DDDDDD;
}

/*=====================================================================*/
/*Ë®ÆœQScrollBar*/
/*=====================================================================*/
QScrollBar:horizontal{
        /*margin:0px 0px;*/ /*Áô¿ÕŒäžøÁœÍ·µÄÐ¡ŒýÍ·²¿Œþ*/
    border-radius:4px;
    max-height:16px;
        background:#DDDDDD;
}

QScrollBar::handle:horizontal{
    min-width:50px;
    border-radius:4px;
        background:#ADAEAF;
}

QScrollBar::handle:horizontal:hover{
    background:#ADAEAF;
}

QScrollBar::handle:horizontal:pressed{
    background:#F8CE72;
}

QScrollBar::add-page:horizontal{
    background:none;
}

QScrollBar::sub-page:horizontal{
    background:none;
}

QScrollBar::add-line:horizontal{
    background:none;
}

QScrollBar::sub-line:horizontal{
    background:none;
}

