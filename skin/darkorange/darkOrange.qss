/*=====================================================================*/
/*QMainWindow*/
/*=====================================================================*/
QMainWindow{

}
QMainWindow::separator {
    width: 3px; /* when vertical */
    height: 3px; /* when horizontal */
    background:#242A28;
}
QMainWindow::separator:hover {
    background: #E5D475;
}

/*=====================================================================*/
