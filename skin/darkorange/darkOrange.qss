/*=====================================================================*/
/*项视图/
/*=====================================================================*/
QTreeView,
QListView,
QTableView{
    background:#6A6E73;
    show-decoration-selected:1;
}

QTableView::item,
QListView::item,
QTreeView::item{
    /*color:#FFFFFF;
    background:#6A6E73;*/
    border:1px solid transparent;
    padding:1px;
    margin:0px;
}

QTableView::item:selected,
QListView::item:selected,
QTreeView::item:selected{
    color:#000000;
    background:#F8CE72;
    border:1px solid transparent;
}

QTableView::item:hover,
QListView::item:hover,
QTreeView::item:hover{
    color:#000000;
    background:#F8CE72;
    border:1px solid transparent;
}

QTableView::item:disabled,
QListView::item:disabled,
QTreeView::item:disabled{
    color:#8D8D8D;/*多余的，由前面*统一设置*/
}

QListView,
QTableView::pane{
    border:1px solid #242A28;
}

QHeaderView{
    border:0px solid #242A28;
    color:#FFFFFF;
    background:#DDDDDD;
}

QHeaderView::section,
QTableCornerButton:section{
    padding:3px;
    margin:0px;
    color:#000000;
    background:#DDDDDD;
    border:1px solid #242A28;
}

QTreeView
{
    alternate-background-color: transparent;
}

