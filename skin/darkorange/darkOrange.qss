}

/*=====================================================================*/
/*QToolButton*/
/*=====================================================================*/
QToolButton{
    min-height:1.2em;
    border:1px solid #242A28;
    color:#FFFFFF;
    background:#6A6E73;
}
QToolButton:checked{
    background:#FFB56B;
}

.QToolButton:hover{
    border:1px solid #242A28;
    color:#000000;
    background-color:#F8CE72;
}

.QToolButton:pressed{
    border:1px solid #242A28;
    color:#000000;
    background-color:#FFB56B;
}

.QToolButton:disabled{
    border:1px solid #54575B;
}

QToolButton::menu-indicator{
    image:None;
}

QToolButton:default{
    border:2px solid #242A28;
}


/*=====================================================================*/
/*QLabel*/
/*=====================================================================*/
QLabel{
    color: #FFFFFF;
        background: transparent;
}


/*=====================================================================*/
/*Input widgets*/
/*=====================================================================*/
QLineEdit,QTextEdit,QPlainTextEdit,QSpinBox,
QDoubleSpinBox,QDateEdit,QTimeEdit,QDateTimeEdit{
    border:1px solid #242A28;
    border-radius:1px;
    padding:2px;
    background:#FDFDFD;
        color:#000000;
    selection-background-color:#F8CE72;
    selection-color:#000000;
}

QLineEdit:focus,QTextEdit:focus,
QPlainTextEdit:focus,QSpinBox:focus,
QDoubleSpinBox:focus,
QDateEdit:focus,QTimeEdit:focus,
QDateTimeEdit:focus{
    border:1px solid #F8CE72; /*µÃµœœ¹µã*/
}

QLineEdit:hover,QTextEdit:hover,
QPlainTextEdit:hover,QSpinBox:hover,
QDoubleSpinBox:hover,
QDateEdit:hover,QTimeEdit:hover,QDateTimeEdit:hover{
    border:1px solid #F8CE72;
}

