/*透明控件*/
QWidget[QSSTransparent="true"]{
    border:none;
    background-color:transparent;
}


/*ribbon工具栏按钮*/
QToolButton[RibbonButton="true"]{
    color:#FFF;
    background:transparent;
    border:1px solid transparent;
}

/*ribbon工具栏面板*/
QWidget[ribbonBox="true"]{
    border:0px solid #000;
    border-right: 1px solid #666;
    border-radius:0px;
    background:transparent;
}

/*ribbon工具栏面板标题*/
QWidget[ribbonBox="true"] QLabel{
    color: #AAA;
    font-size:0.8em;
}

QWidget[ribbonArea="true"]{
/*    min-height:4em;
    max-height:4em;*/
}

QWidget[toolBarWidget="true"]{
    margin: 0px;
    border: 0px;
    padding: 0px;
   /* background:#EAF7FF;*/
}
QWidget[visualToolbar="true"]{
    margin: 0px;
    border: 0px;
    padding: 5px;
    /*background:#EAF7FF;*/
}
QWidget[visualToolbar="true"] QToolButton{
    border-style:none;
    min-height:32px;
    max-height:32px;
    min-width:32px;
    max-width:32px;
    /*background:#EAF7FF;*/
}
QWidget[visualToolbar="true"] QToolButton::checked{
    background-color:#FFB56B;
}




