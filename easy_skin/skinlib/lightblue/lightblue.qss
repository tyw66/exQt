    height:0.7em;
    border:1px solid #C0DCF2;
}

QCheckBox::indicator:hover{
    border:1px solid #336688;
    background:#B3DDFF;
}

/*********************
* 菜单
**********************/
QMenuBar::item{
/*    color:#336688;*/
    background-color:#DEF0FE;
    margin:0px;
